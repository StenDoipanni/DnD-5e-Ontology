###  http://www.ontologydesignpatterns.org/ont/dnd5e/classes#all-armor
:all-armor rdf:type owl:NamedIndividual ,
                    :DnD_Entity ;
           :url "/api/proficiencies/all-armor" ;
           rdfs:label "All armor" .


###  http://www.ontologydesignpatterns.org/ont/dnd5e/classes#arrow
:arrow rdf:type owl:NamedIndividual ,
                :DnD_Entity ;
       :quantity 20 ;
       :url "/api/equipment/arrow" ;
       rdfs:label "Arrow" .


###  http://www.ontologydesignpatterns.org/ont/dnd5e/classes#bagpipes
:bagpipes rdf:type owl:NamedIndividual ,
                   :DnD_Entity ;
          :optionType "reference" ;
          :url "/api/proficiencies/bagpipes" ;
          rdfs:label "Bagpipes" .


###  http://www.ontologydesignpatterns.org/ont/dnd5e/classes#barbarian
:barbarian rdf:type owl:NamedIndividual ,
                    :DnD_Entity ;
           :hasProficiency :light-armor ,
                           :martial-weapons ,
                           :medium-armor ,
                           :saving-throw-con ,
                           :saving-throw-str ,
                           :shields ,
                           :simple-weapons ;
           :hasProficiencyChoice :barbarian_proficiency_choice_1 ;
           :hasSavingThrow :con ,
                           :str ;
           :hasStartingEquipment :barbarian_starting_equipment ;
           :hasStartingEquipmentOptions :barbarian_starting_equipment_options ;
           :hit_die "d12" ;
           rdfs:label "Barbarian" .


###  http://www.ontologydesignpatterns.org/ont/dnd5e/classes#barbarian_proficiency_choice_1
:barbarian_proficiency_choice_1 rdf:type owl:NamedIndividual ,
                                         :DnD_Entity ;
                                :hasOptionsArray :barbarian_proficiency_choice_1_options_array ;
                                :choose 2 ;
                                :type "proficiencies" ;
                                rdfs:comment "Choose two from Animal Handling, Athletics, Intimidation, Nature, Perception, and Survival" .


###  http://www.ontologydesignpatterns.org/ont/dnd5e/classes#barbarian_proficiency_choice_1_options_array
:barbarian_proficiency_choice_1_options_array rdf:type owl:NamedIndividual ,
                                                       :DnD_Entity ;
                                              :hasChoice :barbarian_starting_equipment_option_1_choice_2 ,
                                                         :barbarian_starting_equipment_option_2_choice_2 ;
                                              :hasOption :greataxe ,
                                                         :handaxe ,
                                                         :skill-animal-handling ,
                                                         :skill-athletics ,
                                                         :skill-intimidation ,
                                                         :skill-nature ,
                                                         :skill-perception ,
                                                         :skill-survival .


